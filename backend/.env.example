# API Keys
KALIBR_API_KEY=your_kalibr_api_key_here
KALIBR_TENANT_ID=your_kalibr_tenant_id_here
CRYPTORACLE_API_KEY=your_cryptoracle_api_key_here
COINGECKO_API_KEY=your_coingecko_api_key_here

# Optional: override provider base URLs
CRYPTORACLE_BASE_URL=https://service.cryptoracle.network

# Cryptoracle realtime data has ~10 minute latency (docs). Backend queries endTime behind "now".
CRYPTORACLE_DATA_LATENCY_MS=600000

# Kalibr model IDs (set these to Gemini model IDs supported by Kalibr)
KALIBR_MODEL_HIGH=gemini-2.0-flash
KALIBR_MODEL_LOW=gemini-2.0-flash

# If Kalibr routes to a model your Gemini key can't access, backend will fall back.
GEMINI_MODEL_FALLBACKS=gemini-2.5-flash,gemini-2.0-flash,gemini-2.0-flash-lite,gemini-1.5-flash

# Provider key for Gemini (Kalibr routes, but calls go to provider directly)
# Kalibr docs name this GOOGLE_API_KEY; GEMINI_API_KEY is accepted as an alias here.
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Thresholds
SENTIMENT_BAD_THRESHOLD=30
RISK_THRESHOLD_DEFAULT=80

# Blockchain
EVM_RPC_URL=https://bsc-testnet-rpc.publicnode.com
BSC_RPC_URL=https://bsc-dataseed.binance.org/
PRIVATE_KEY=your_guardian_private_key_here

# VibeShield Vault (non-custodial autopilot)
VIBESHIELD_VAULT_ADDRESS=0xYourDeployedVault

# PancakeSwap Router
PANCAKESWAP_ROUTER=0x10ED43C718714eb63d5aA57B78B54704E256024E

# Monitor loop
ENABLE_MONITOR=false
MONITOR_INTERVAL_MS=30000

# Debug endpoints (keep false in production)
DEBUG=false

# Server
PORT=3000
NODE_ENV=development

# Telegram/Discord Notifications (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id
