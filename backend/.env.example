# API Keys
KALIBR_API_KEY=your_kalibr_api_key_here
CRYPTORACLE_API_KEY=your_cryptoracle_api_key_here
COINGECKO_API_KEY=your_coingecko_api_key_here

# Kalibr model IDs (set these to Gemini model IDs supported by Kalibr)
# Example placeholders (check Kalibr docs for exact names): gemini-2.0-flash, gemini-1.5-pro
KALIBR_MODEL_HIGH=gemini-1.5-pro
KALIBR_MODEL_LOW=gemini-2.0-flash

# Optional: if your Kalibr setup needs provider keys
GEMINI_API_KEY=your_gemini_api_key_here

# Thresholds
SENTIMENT_BAD_THRESHOLD=30
RISK_THRESHOLD_DEFAULT=80

# Blockchain
BSC_RPC_URL=https://bsc-dataseed.binance.org/
PRIVATE_KEY=your_guardian_private_key_here

# VibeGuard Vault (non-custodial autopilot)
VIBEGUARD_VAULT_ADDRESS=0xYourDeployedVault

# PancakeSwap Router
PANCAKESWAP_ROUTER=0x10ED43C718714eb63d5aA57B78B54704E256024E

# Monitor loop
ENABLE_MONITOR=false
MONITOR_INTERVAL_MS=30000

# Server
PORT=3000
NODE_ENV=development

# Telegram/Discord Notifications (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id
